public class IndexWeightNode 
{
	public int[] index;
	public int count = 0;
	public int n;
	public double[] weight;
	public IndexWeightNode(int e)
	{
		iwn[count].n = e;
    	for(int y=0;y<e;y++)
    	{
    		iwn[count].index = new int[e];
    		iwn[count].weight = new double[e];
    	}
	}
}